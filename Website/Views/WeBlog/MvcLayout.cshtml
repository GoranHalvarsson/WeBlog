    @Html.Sitecore().Rendering("{CFBDF43B-D3D4-4F5F-BDE8-BFDA3EDA3778}", new {Cacheable = true, CacheKey = "BlogSyndication", Cache_VaryByData = true})
    @Html.Sitecore().Rendering("{9AF6BA6B-221F-4549-93C5-B66DB5DF6429}", new {Cacheable = true, CacheKey = "RsdIncludes", Cache_VaryByData = true})
